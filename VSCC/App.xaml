<Application x:Class="VSCC.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VSCC"
             xmlns:skins="clr-namespace:VSCC.Skins"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <skins:SkinResourceDictionary DarkSource="Skins/Dark.xaml" DefaultSource="Skins/Default.xaml" SoftSource="Skins/Soft.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
